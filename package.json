{
  "name": "thumble",
  "productName": "thumble",
  "version": "1.0.4",
  "description": "thumble",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "postinstall": "electron-builder install-app-deps",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Buruss",
    "email": "buruss@ymail.com"
  },
  "license": "MIT",
  "prisma": {
    "seed": "ts-node src/main/db/seed.ts"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "resources/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "thumble"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-s3",
          "platforms": [
            "win32"
          ],
          "config": {
            "region": "ap-northeast-2",
            "bucket": "npro",
            "folder": "thumble",
            "public": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "name": "login_window",
                  "html": "./src/renderer/static/html/index.html",
                  "js": "./src/renderer/login/index.tsx",
                  "preload": {
                    "js": "./src/renderer/preload.ts"
                  }
                },
                {
                  "name": "main_window",
                  "html": "./src/renderer/static/html/index.html",
                  "js": "./src/renderer/main/index.tsx",
                  "preload": {
                    "js": "./src/renderer/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@electron-forge/publisher-s3": "^6.0.0-beta.61",
    "@nestjs/testing": "^8.2.3",
    "@types/cron": "^1.7.3",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.9",
    "@types/puppeteer": "^5.4.4",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.0.0",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "prisma": "^3.6.0",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.5.2",
    "unlazy-loader": "^0.1.3",
    "zod-prisma": "^0.3.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@nestjs/common": "^8.2.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/microservices": "^8.2.3",
    "@nestjs/schedule": "^1.0.2",
    "@prisma/client": "^3.6.0",
    "@trpc/client": "^9.14.0",
    "@trpc/react": "^9.14.0",
    "@trpc/server": "^9.14.0",
    "ag-grid-community": "^26.2.0",
    "ag-grid-react": "^26.2.0",
    "antd": "^4.17.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.10.7",
    "electron-squirrel-startup": "^1.0.0",
    "lodash": "^4.17.21",
    "nest-winston": "^1.6.1",
    "puppeteer-core": "^12.0.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.33.4",
    "react-router-dom": "^6.0.2",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "zod": "^3.11.6"
  }
}
